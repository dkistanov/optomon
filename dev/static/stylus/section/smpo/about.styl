.smpo-about__head
  display flex
  justify-content space-between
  margin-bottom 35px
  svg
    width 145px
    height 100px

.smpo-about__info
  max-width 375px

.smpo-about__title
  margin-bottom 30px

.smpo-about__list
  max-width 620px
  margin-bottom -30px

.smpo-about__item
  width 100%
  margin-bottom 30px
  &:nth-child(odd)
    max-width 230px
  svg
    margin-right 10px
    width 40px
    height 40px
  @media (max-width breakpoint-md)
    &:nth-child(odd)
      max-width 50%
  @media (max-width breakpoint-sm)
    &:nth-child(odd)
      max-width 100%
